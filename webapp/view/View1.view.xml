<mvc:View xmlns:cards="sap.f.cards" controllerName="day6css.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <customHeader>
            <Bar id="bar0" class="headerBar">
                <contentMiddle>
                    <Label id="label0" text="Page 1 Header" class="headerTitle"/>
                </contentMiddle>
            </Bar>
        </customHeader>
        <footer>
            <Bar id="bar1" class="footerBar">
                <contentRight>
                    <Button id="btn0" text="Info" press="onInfoPress"/>
                    <Button id="btn1" text="Next" press="onNextPress"/>
                </contentRight>
            </Bar>
        </footer>
    </Page>
</mvc:View>